<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Likert_Respondido">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:id="@+id/fg_menu"
            class="com.educa.FragmentMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout="@layout/fragment_menu" />

        <View
            android:id="@+id/rectangle_2"
            android:layout_width="319dp"
            android:layout_height="510dp"
            android:layout_below="@+id/fg_menu"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="83dp"
            android:background="@drawable/db_card_rectangle_likert"

            />

        <View
            android:id="@+id/rectangle2_likert"
            android:layout_width="7dp"
            android:layout_height="476dp"
            android:layout_below="@id/fg_menu"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="43dp"
            android:layout_marginTop="104dp"
            android:background="@drawable/db_card_rectangle2_likert" />

        <ImageView
            android:id="@+id/image_35"
            android:layout_width="211dp"
            android:layout_height="121dp"
            android:layout_below="@id/fg_menu"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="200dp"
            android:background="@mipmap/img_likert_respondido" />

        <TextView
            android:id="@+id/obrigado_a_"
            android:layout_width="234dp"
            android:layout_height="40dp"
            android:layout_below="@id/image_35"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="43dp"
            android:fontFamily="@font/montserrat_bold"
            android:text="@string/txt_tks_avaliable1"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/txt_tks"
            android:layout_width="220dp"
            android:layout_height="60dp"
            android:layout_below="@id/obrigado_a_"
            android:layout_marginLeft="120dp"
            android:text="@string/txt_tks_avaliable2"
            android:fontFamily="@font/montserrat"
            />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_go_quiz"
            style="@style/style_btnYellowSmall"
            android:layout_below="@+id/txt_tks"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/db_btn_solid"
            android:text="@string/txt_access_certificate"
            android:textSize="9sp"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_back"
            style="@style/style_btnBorderPurpleSmall"
            android:layout_below="@+id/btn_go_quiz"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="6.03dp"
            android:background="@drawable/db_btn_border"
            android:text="@string/txt_back_start"
            android:textColor="@color/black"
            android:textSize="9sp"
            tools:ignore="TouchTargetSizeCheck" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>